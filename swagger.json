                    "recommend": { "type": "string" },
                    "page": { "type": "number" },
                    "limit": { "type": "number" },
                    "totalPage": { "type": "number" },
                    "totalCount": { "type": "number" },
                    "results": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "_id": { "type": "string" },
                          "weight": {
                            "type": "integer",
                            "example": "100"
                          },
                          "calories": {
                            "type": "integer",
                            "example": "300"
                          },
                          "category": {
